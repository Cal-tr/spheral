TableKernel = Template("Spheral::KernelSpace::TableKernel",
                       "ExtendTableKernel.hh")

exclude(TableKernel.asDescendent)
exclude(TableKernel.nperhValues)
exclude(TableKernel.WsumValues)

final(TableKernel.valid)

TableKernel1d = TableKernel("Spheral::Dim<1>", "TableKernel1d")
add_method(TableKernel1d, "Spheral::nperhValues1d")
add_method(TableKernel1d, "Spheral::WsumValues1d")
rename(TableKernel1d.nperhValues1d, "nperh")
rename(TableKernel1d.WsumValues1d, "Wsum")

