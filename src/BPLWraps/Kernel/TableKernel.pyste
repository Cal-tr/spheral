add_method(TableKernel2d, "Spheral::nperhValues2d")
add_method(TableKernel2d, "Spheral::WsumValues2d")
rename(TableKernel2d.nperhValues2d, "nperh")
rename(TableKernel2d.WsumValues2d, "Wsum")

