exclude(ngdb.addNodeList)

ngdb1d = ngdb("Spheral::Dim<1>", "NestedGridDistributedBoundary1d")
ngdb2d = ngdb("Spheral::Dim<2>", "NestedGridDistributedBoundary2d")
ngdb3d = ngdb("Spheral::Dim<3>", "NestedGridDistributedBoundary3d")
