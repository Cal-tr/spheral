Exported("Spheral::BoundarySpace::Boundary<Spheral::Dim<1> >")
Exported("Spheral::BoundarySpace::Boundary<Spheral::Dim<2> >")
Exported("Spheral::BoundarySpace::Boundary<Spheral::Dim<3> >")

ngdb = Template("Spheral::BoundarySpace::NestedGridDistributedBoundary",
                "ExtendNestedGridDistributedBoundary.hh")

set_policy(ngdb.instance, return_value_policy(reference_existing_object))
