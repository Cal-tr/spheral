# -*-makefile-*-
# Spheral++ Material pybind11 package makefile
#-------------------------------------------------------------------------------
srcdir = @abs_srcdir@
VPATH = @abs_srcdir@
SPHERALTOP = @abs_top_srcdir@
BUILDTOP = @abs_top_builddir@

PKGNAME = Material
PKGDIR = $(PKGNAME)/
PYB11SRCS = \
	PhysicalConstants.py \
	EquationOfState.py \
	GammaLawGas.py \
	PolytropicEquationOfState.py \
	IsothermalEquationOfState.py
PYB11TARGET = Spheral$(PKGNAME).$(SHLIBEXT)

#------------------------------------------------------------------------------
include $(BUILDTOP)/helpers/makefile_master
EXTRALIBS += $(LDRPATH)
