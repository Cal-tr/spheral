srcdir = @abs_srcdir@
VPATH = @abs_srcdir@
SPHERALTOP = @abs_top_srcdir@
BUILDTOP = @abs_top_builddir@

PKGNAME = Geometry
PKGDIR = $(PKGNAME)/
PYB11SRCS = \
	Vector.py \
	Tensor.py \
	ThirdRankTensor.py \
	FourthRankTensor.py \
	FifthRankTensor.py
PYB11TARGET = Spheral$(PKGNAME).$(SHLIBEXT)

#------------------------------------------------------------------------------
include $(BUILDTOP)/helpers/makefile_master
EXTRALIBS += $(LDRPATH)
