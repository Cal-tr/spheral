# -*-makefile-*-
# Spheral++ Geometry pybind11 package makefile
#-------------------------------------------------------------------------------
srcdir = @abs_srcdir@
VPATH = @abs_srcdir@
SPHERALTOP = @abs_top_srcdir@
BUILDTOP = @abs_top_builddir@

PKGNAME = Geometry
PKGDIR = $(PKGNAME)/
PYB11SRCS = \
	Vector.py \
	Tensor.py \
	SymTensor.py \
	ThirdRankTensor.py \
	FourthRankTensor.py \
	FifthRankTensor.py \
	EigenStruct.py \
	Plane.py \
	Box1d.py \
	Polygon.py \
	Polyhedron.py \
	Facet2d.py \
	Facet3d.py

PYB11TARGET = Spheral$(PKGNAME).$(SHLIBEXT)

#------------------------------------------------------------------------------
include $(BUILDTOP)/helpers/makefile_master
EXTRALIBS += $(LDRPATH)
