# -*-makefile-*-
# Spheral++ ArtificialViscosity pybind11 package makefile
#-------------------------------------------------------------------------------
srcdir = @abs_srcdir@
VPATH = @abs_srcdir@
SPHERALTOP = @abs_top_srcdir@
BUILDTOP = @abs_top_builddir@

PKGNAME = ArtificialViscosity
PKGDIR = $(PKGNAME)/
PYB11TARGET = Spheral$(PKGNAME).$(SHLIBEXT)
PYB11SRCS += \
	$(srcdir)/ArtificialViscosityAbstractMethods.py \
	$(srcdir)/ArtificialViscosity.py \
	$(srcdir)/MonaghanGingoldViscosity.py \
	$(srcdir)/TensorMonaghanGingoldViscosity.py \
	$(srcdir)/CRKSPHMonaghanGingoldViscosity.py \
	$(srcdir)/MorrisMonaghanReducingViscosity.py \
	$(srcdir)/CullenDehnenViscosity.py \
	$(srcdir)/FiniteVolumeViscosity.py \
	$(srcdir)/TensorSVPHViscosity.py

#------------------------------------------------------------------------------
include $(BUILDTOP)/helpers/makefile_master
EXTRALIBS += $(LDRPATH)
