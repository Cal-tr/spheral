# -*-makefile-*-
# Spheral++ Distributed pybind11 package makefile
#-------------------------------------------------------------------------------
srcdir = @abs_srcdir@
VPATH = @abs_srcdir@
SPHERALTOP = @abs_top_srcdir@
BUILDTOP = @abs_top_builddir@

PKGNAME = Distributed
PKGDIR = $(PKGNAME)/
PYB11TARGET = Spheral$(PKGNAME).$(SHLIBEXT)
PYB11SRCS = \
	DistributedBoundary.py \
	NestedGridDistributedBoundary.py \
	BoundingVolumeDistributedBoundary.py \
	TreeDistributedBoundary.py

#------------------------------------------------------------------------------
include $(BUILDTOP)/helpers/makefile_master
EXTRALIBS += $(LDRPATH)
